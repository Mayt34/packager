{
  "Env": {},
  "DependsOn": ["spdlog"],
  "Git": {
    "URI": "https://github.com/bringauto/ba-logger.git",
    "Revision": "v1.2.0"
  },
  "Build": {
    "CMake": {
      "Defines": {
        "CMAKE_BUILD_TYPE": "Release",
        "LIB_TYPE": "SPDLOG",
        "BRINGAUTO_INSTALL": "ON",
        "BRINGAUTO_SYSTEM_DEP": "ON"
      }
    }
  },
  "Package": {
    "Name": "ba-logger",
    "VersionTag": "v1.2.0",
    "PlatformString": {
      "Mode": "auto"
    },
    "IsLibrary": true,
    "IsDevLib": true,
    "IsDebug": false
  },
  "DockerMatrix": {
    "ImageNames": [
      "ubuntu2004",
      "debian11",
      "ubuntu2204"
    ]
  }
}
