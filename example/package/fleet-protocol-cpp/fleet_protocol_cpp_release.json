{
  "Env": {},
  "DependsOn": [
    "fleet-protocol-interface"
  ],
  "Git": {
    "URI": "https://github.com/bringauto/fleet-protocol-cpp.git",
    "VersionTag": "v1.1.0"
  },
  "Build": {
    "CMake": {
      "Defines": {
        "CMAKE_BUILD_TYPE": "Release",
        "BRINGAUTO_INSTALL": "ON",
        "BRINGAUTO_PACKAGE": "ON",
        "BRINGAUTO_SYSTEM_DEP": "ON",
        "BRINGAUTO_TESTS" : "OFF"
      }
    }
  },
  "Package": {
    "Name": "fleet-protocol-cxx-helpers-static",
    "VersionTag": "v1.1.0",
    "PlatformString": {
      "Mode": "auto"
    },
    "IsLibrary": true,
    "IsDevLib": true,
    "IsDebug": false
  },
  "DockerMatrix": {
    "ImageNames": [
      "ubuntu2004",
      "ubuntu2204",
      "debian12",
      "debian11",
      "ubuntu2304",
      "ubuntu2310"
    ]
  }
}
