{
  "Env": {},
  "DependsOn": ["ba-logger", "cxxopts"],
  "Git": {
    "URI": "https://github.com/Melky-Phoe/StateSmurf.git",
    "Revision": "v2.0.0"
  },
  "Build": {
    "CMake": {
      "Defines": {
        "CMAKE_BUILD_TYPE": "Debug",
        "BRINGAUTO_INSTALL": "ON",
        "BRINGAUTO_PACKAGE": "ON",
        "BRINGAUTO_SYSTEM_DEP": "ON"
      }
    }
  },
  "Package": {
    "Name": "statesmurf",
    "VersionTag": "v2.0.0",
    "PlatformString": {
      "Mode": "auto"
    },
    "IsLibrary": true,
    "IsDevLib": true,
    "IsDebug": true
  },
  "DockerMatrix": {
    "ImageNames": [
    	"ubuntu2004",
    	"debian11",
    	"ubuntu2204"
    ]
  }
}
